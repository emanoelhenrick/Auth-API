{
  "name": "auth-api",
  "version": "1.2.3",
  "description": "A Authentication API made with clean architecture and DDD",
  "main": "src/main/server.ts",
  "scripts": {
    "dev": "NODE_ENV=dev tsx watch src/main/server.ts",
    "dev:docker": "nodemon -L --watch ./dist ./dist/main/server.js",
    "up": "npm run build && docker compose up -d",
    "down": "docker compose down",
    "start": "node dist/main/server.js",
    "debug": "node --inspect=0.0.0.0:10500 --nolazy dist/main/server.js",
    "build": "rimraf dist && tsc",
    "test": "NODE_ENV=dev vitest",
    "test:unit": "NODE_ENV=dev vitest -c vitest-unit.config.ts",
    "test:integration": "NODE_ENV=dev vitest -c vitest-integration.config.ts",
    "test:coverage": "NODE_ENV=dev vitest --coverage"
  },
  "keywords": [],
  "author": "Emanoel Henrick",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.35",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@vitest/coverage-c8": "^0.32.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.31.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "nodemon": "^1.14.9",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
